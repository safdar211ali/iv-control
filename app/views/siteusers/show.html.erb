<div class="panel panel-info">
  <!-- Default panel contents -->
  <div class="panel-heading">
<%- model_class = User -%>
  <h3><%= 'Show' %> | <%=t '.title', :default => model_class.model_name.human.titleize %></h3>
  </div>
  <div class="panel-body">

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:email) %></dt>
      <dd><pre class="prettyprint"><%= @siteuser.email %></pre></dd>
    <dt class="label label-default"><%= model_class.human_attribute_name(:role) %></dt>
    <dd><pre class="prettyprint"><%= @siteuser.role %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:current_sign_in_at) %></dt>
      <dd><pre class="prettyprint"><%= @siteuser.current_sign_in_at %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:last_sign_in_at) %></dt>
      <dd><pre class="prettyprint"><%= @siteuser.last_sign_in_at %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:current_sign_in_ip) %></dt>
      <dd><pre class="prettyprint"><%= @siteuser.current_sign_in_ip %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:last_sign_in_ip) %></dt>
      <dd><pre class="prettyprint"><%= @siteuser.last_sign_in_ip %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              siteusers_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_siteuser_path(@siteuser), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              siteuser_path(@siteuser),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
  </div>

</div>
